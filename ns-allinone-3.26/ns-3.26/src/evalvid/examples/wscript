## -*- Mode: python; py-indent-offset: 4; indent-tabs-mode: nil; coding: utf-8; -*-

def build(bld):
    obj = bld.create_ns3_program('evalvid-client-server', ['csma', 'evalvid'])
    obj.source = 'evalvid-client-server.cc'

    obj = bld.create_ns3_program('evalvid-lte', ['lte', 'evalvid'])
    obj.source = 'evalvid-lte.cc'

